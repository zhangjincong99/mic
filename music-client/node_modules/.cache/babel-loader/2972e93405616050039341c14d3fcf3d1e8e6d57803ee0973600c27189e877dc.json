{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { HttpManager } from \"@/api\";\nexport default {\n  data() {\n    return {\n      ranking: [] // 评分排行榜\n    };\n  },\n\n  mounted() {\n    this.getSongListRanking();\n  },\n  methods: {\n    async getSongListRanking() {\n      const songList = await HttpManager.getSongList(); // 获取全部歌单\n\n      for (const list of songList) {\n        const rank = await HttpManager.getRankOfSongListId(list.id); // 获取指定歌单的评分\n        let totalScore = 0;\n        let averageScore = 0;\n        if (rank.length > 0) {\n          for (const r of rank) {\n            totalScore += r.score;\n          }\n          averageScore = totalScore / rank.length;\n        }\n        this.ranking.push({\n          songListId: list.id,\n          title: list.title,\n          score: averageScore\n        });\n      }\n      this.ranking.sort((a, b) => b.score - a.score); // 按评分降序排序\n    }\n  }\n};","map":{"version":3,"names":["HttpManager","data","ranking","mounted","getSongListRanking","methods","songList","getSongList","list","rank","getRankOfSongListId","id","totalScore","averageScore","length","r","score","push","songListId","title","sort","a","b"],"sources":["E:\\javaideaiu\\class22\\mic\\music-client\\src\\views\\song-sheet\\paihangbang.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>歌单评分排行榜</h2>\r\n    <ul>\r\n      <li v-for=\"item in ranking\" :key=\"item.songListId\">\r\n        <span>{{ item.title }}</span>\r\n        <span>评分：{{ item.score }}</span>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { HttpManager } from \"@/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      ranking: [], // 评分排行榜\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getSongListRanking();\r\n  },\r\n  methods: {\r\n    async getSongListRanking() {\r\n      const songList = await HttpManager.getSongList(); // 获取全部歌单\r\n\r\n      for (const list of songList) {\r\n        const rank = await HttpManager.getRankOfSongListId(list.id); // 获取指定歌单的评分\r\n        let totalScore = 0;\r\n        let averageScore = 0;\r\n\r\n        if (rank.length > 0) {\r\n          for (const r of rank) {\r\n            totalScore += r.score;\r\n          }\r\n\r\n          averageScore = totalScore / rank.length;\r\n        }\r\n\r\n        this.ranking.push({\r\n          songListId: list.id,\r\n          title: list.title,\r\n          score: averageScore,\r\n        });\r\n      }\r\n\r\n      this.ranking.sort((a, b) => b.score - a.score); // 按评分降序排序\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AAYA,SAASA,WAAU,QAAS,OAAO;AAEnC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE,CAAE;IACf,CAAC;EACH,CAAC;;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,kBAAkB,CAAC,CAAC;EAC3B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,kBAAkBA,CAAA,EAAG;MACzB,MAAME,QAAO,GAAI,MAAMN,WAAW,CAACO,WAAW,CAAC,CAAC,EAAE;;MAElD,KAAK,MAAMC,IAAG,IAAKF,QAAQ,EAAE;QAC3B,MAAMG,IAAG,GAAI,MAAMT,WAAW,CAACU,mBAAmB,CAACF,IAAI,CAACG,EAAE,CAAC,EAAE;QAC7D,IAAIC,UAAS,GAAI,CAAC;QAClB,IAAIC,YAAW,GAAI,CAAC;QAEpB,IAAIJ,IAAI,CAACK,MAAK,GAAI,CAAC,EAAE;UACnB,KAAK,MAAMC,CAAA,IAAKN,IAAI,EAAE;YACpBG,UAAS,IAAKG,CAAC,CAACC,KAAK;UACvB;UAEAH,YAAW,GAAID,UAAS,GAAIH,IAAI,CAACK,MAAM;QACzC;QAEA,IAAI,CAACZ,OAAO,CAACe,IAAI,CAAC;UAChBC,UAAU,EAAEV,IAAI,CAACG,EAAE;UACnBQ,KAAK,EAAEX,IAAI,CAACW,KAAK;UACjBH,KAAK,EAAEH;QACT,CAAC,CAAC;MACJ;MAEA,IAAI,CAACX,OAAO,CAACkB,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACN,KAAI,GAAIK,CAAC,CAACL,KAAK,CAAC,EAAE;IAClD;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}