{"ast":null,"code":"import \"core-js/modules/web.url-search-params.size.js\";\nimport { ref, computed, onMounted } from \"vue\";\nimport { HttpManager } from \"@/api\";\nexport default {\n  name: \"SongDetails\",\n  data() {\n    return {\n      songDetails: {},\n      rank: 0,\n      score: 0,\n      disabledRank: false,\n      assistText: \"评价\",\n      currentSongList: [],\n      songListId: \"\",\n      userId: \"\"\n    };\n  },\n  computed: {\n    attachImageUrl() {\n      return HttpManager.attachImageUrl;\n    }\n  },\n  methods: {\n    async getSongDetails() {\n      // 获取歌单信息\n      const songDetails = await HttpManager.getSongListDetails(this.songListId);\n      this.songDetails = songDetails.data;\n\n      // 获取歌单评分\n      const rank = await HttpManager.getRankOfSongListId(this.songListId);\n      this.rank = rank.data / 2;\n\n      // 获取用户评分\n      const userRank = await HttpManager.getUserRank(this.userId, this.songListId);\n      if (userRank.data) {\n        this.score = userRank.data / 2;\n        this.disabledRank = true;\n        this.assistText = \"已评价\";\n      }\n    },\n    async pushValue() {\n      if (this.disabledRank) return;\n      const params = new URLSearchParams();\n      params.append(\"songListId\", this.songListId);\n      params.append(\"consumerId\", this.userId);\n      params.append(\"score\", (this.score * 2).toString());\n      try {\n        const result = await HttpManager.setRank(params);\n        // 处理结果\n        if (result.success) {\n          this.getRank(this.songListId);\n          this.disabledRank = true;\n          this.assistText = \"已评价\";\n        }\n      } catch (error) {\n        console.error(error);\n      }\n    }\n  },\n  async mounted() {\n    this.songListId = \"your_song_list_id\";\n    this.userId = \"your_user_id\";\n    await this.getSongDetails();\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","HttpManager","name","data","songDetails","rank","score","disabledRank","assistText","currentSongList","songListId","userId","attachImageUrl","methods","getSongDetails","getSongListDetails","getRankOfSongListId","userRank","getUserRank","pushValue","params","URLSearchParams","append","toString","result","setRank","success","getRank","error","console","mounted"],"sources":["E:\\javaideaiu\\class22\\mic\\music-client\\src\\views\\paihangbang.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <img :src=\"songDetails.pic\" :alt=\"songDetails.title\" />\r\n      <h3>{{ songDetails.title }}</h3>\r\n    </div>\r\n    <div>\r\n      <h1>简介</h1>\r\n      <p>{{ songDetails.introduction }}</p>\r\n    </div>\r\n    <div>\r\n      <h3>歌单评分</h3>\r\n      <el-rate v-model=\"rank\" allow-half disabled></el-rate>\r\n      <span>{{ rank * 2 }}</span>\r\n    </div>\r\n    <div>\r\n      <h3>{{ assistText }} {{ score * 2 }}</h3>\r\n      <el-rate allow-half v-model=\"score\" :disabled=\"disabledRank\" @click=\"pushValue\"></el-rate>\r\n    </div>\r\n    <div>\r\n      <h2>歌曲列表</h2>\r\n      <ul>\r\n        <li v-for=\"song in currentSongList\" :key=\"song.id\">{{ song.title }}</li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed, onMounted } from \"vue\";\r\nimport { HttpManager } from \"@/api\";\r\n\r\nexport default {\r\n  name: \"SongDetails\",\r\n  data() {\r\n    return {\r\n      songDetails: {},\r\n      rank: 0,\r\n      score: 0,\r\n      disabledRank: false,\r\n      assistText: \"评价\",\r\n      currentSongList: [],\r\n      songListId: \"\",\r\n      userId: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    attachImageUrl() {\r\n      return HttpManager.attachImageUrl;\r\n    },\r\n  },\r\n  methods: {\r\n    async getSongDetails() {\r\n      // 获取歌单信息\r\n      const songDetails = await HttpManager.getSongListDetails(this.songListId);\r\n      this.songDetails = songDetails.data;\r\n\r\n      // 获取歌单评分\r\n      const rank = await HttpManager.getRankOfSongListId(this.songListId);\r\n      this.rank = rank.data / 2;\r\n\r\n      // 获取用户评分\r\n      const userRank = await HttpManager.getUserRank(this.userId, this.songListId);\r\n      if (userRank.data) {\r\n        this.score = userRank.data / 2;\r\n        this.disabledRank = true;\r\n        this.assistText = \"已评价\";\r\n      }\r\n    },\r\n    async pushValue() {\r\n      if (this.disabledRank) return;\r\n\r\n      const params = new URLSearchParams();\r\n      params.append(\"songListId\", this.songListId);\r\n      params.append(\"consumerId\", this.userId);\r\n      params.append(\"score\", (this.score * 2).toString());\r\n\r\n      try {\r\n        const result = await HttpManager.setRank(params);\r\n        // 处理结果\r\n        if (result.success) {\r\n          this.getRank(this.songListId);\r\n          this.disabledRank = true;\r\n          this.assistText = \"已评价\";\r\n        }\r\n      } catch (error) {\r\n        console.error(error);\r\n      }\r\n    },\r\n  },\r\n  async mounted() {\r\n    this.songListId = \"your_song_list_id\";\r\n    this.userId = \"your_user_id\";\r\n    await this.getSongDetails();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* 样式省略，根据需要自行添加样式 */\r\n</style>\r\n"],"mappings":";AA6BA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AAC9C,SAASC,WAAU,QAAS,OAAO;AAEnC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CAAC,CAAC;MACfC,IAAI,EAAE,CAAC;MACPC,KAAK,EAAE,CAAC;MACRC,YAAY,EAAE,KAAK;MACnBC,UAAU,EAAE,IAAI;MAChBC,eAAe,EAAE,EAAE;MACnBC,UAAU,EAAE,EAAE;MACdC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDZ,QAAQ,EAAE;IACRa,cAAcA,CAAA,EAAG;MACf,OAAOX,WAAW,CAACW,cAAc;IACnC;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACrB;MACA,MAAMV,WAAU,GAAI,MAAMH,WAAW,CAACc,kBAAkB,CAAC,IAAI,CAACL,UAAU,CAAC;MACzE,IAAI,CAACN,WAAU,GAAIA,WAAW,CAACD,IAAI;;MAEnC;MACA,MAAME,IAAG,GAAI,MAAMJ,WAAW,CAACe,mBAAmB,CAAC,IAAI,CAACN,UAAU,CAAC;MACnE,IAAI,CAACL,IAAG,GAAIA,IAAI,CAACF,IAAG,GAAI,CAAC;;MAEzB;MACA,MAAMc,QAAO,GAAI,MAAMhB,WAAW,CAACiB,WAAW,CAAC,IAAI,CAACP,MAAM,EAAE,IAAI,CAACD,UAAU,CA