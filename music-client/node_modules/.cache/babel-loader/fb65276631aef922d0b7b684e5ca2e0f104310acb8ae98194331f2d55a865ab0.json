{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      rankList: [] // 存储评分排行榜数据\n    };\n  },\n\n  mounted() {\n    this.getRankList(); // 在组件挂载后获取评分排行榜数据\n  },\n\n  methods: {\n    async getRankList() {\n      // 调用 API 获取全部歌单\n      const songListResponse = await axios.get(`/songList`);\n      const songList = songListResponse.data;\n\n      // 遍历歌单，获取评分并排序\n      const rankList = [];\n      for (const song of songList) {\n        const rankResponse = await axios.get(`/rankList?songListId=${song.songListId}`);\n        const rank = rankResponse.data;\n        rankList.push({\n          songListId: song.songListId,\n          songListName: song.songListName,\n          rank: rank\n        });\n      }\n\n      // 根据评分排序\n      rankList.sort((a, b) => b.rank - a.rank);\n\n      // 更新组件的 rankList 数据\n      this.rankList = rankList;\n    }\n  }\n};","map":{"version":3,"names":["axios","data","rankList","mounted","getRankList","methods","songListResponse","get","songList","song","rankResponse","songListId","rank","push","songListName","sort","a","b"],"sources":["E:\\javaideaiu\\class22\\mic\\music-client\\src\\views\\song-sheet\\paihangbang.vue"],"sourcesContent":["<template>\r\n  <!-- 在模板中显示评分排行榜 -->\r\n  <div>\r\n    <h2>评分排行榜</h2>\r\n    <ul>\r\n      <li v-for=\"item in rankList\" :key=\"item.songListId\">\r\n        <span>{{ item.songListName }}</span>\r\n        <span>评分：{{ item.rank }}</span>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      rankList: [] // 存储评分排行榜数据\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getRankList(); // 在组件挂载后获取评分排行榜数据\r\n  },\r\n  methods: {\r\n    async getRankList() {\r\n      // 调用 API 获取全部歌单\r\n      const songListResponse = await axios.get(`/songList`);\r\n      const songList = songListResponse.data;\r\n\r\n      // 遍历歌单，获取评分并排序\r\n      const rankList = [];\r\n      for (const song of songList) {\r\n        const rankResponse = await axios.get(`/rankList?songListId=${song.songListId}`);\r\n        const rank = rankResponse.data;\r\n\r\n        rankList.push({\r\n          songListId: song.songListId,\r\n          songListName: song.songListName,\r\n          rank: rank\r\n        });\r\n      }\r\n\r\n      // 根据评分排序\r\n      rankList.sort((a, b) => b.rank - a.rank);\r\n\r\n      // 更新组件的 rankList 数据\r\n      this.rankList = rankList;\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":";AAcA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAC,CAAE;IACf,CAAC;EACH,CAAC;;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC,EAAE;EACtB,CAAC;;EACDC,OAAO,EAAE;IACP,MAAMD,WAAWA,CAAA,EAAG;MAClB;MACA,MAAME,gBAAe,GAAI,MAAMN,KAAK,CAACO,GAAG,CAAE,WAAU,CAAC;MACrD,MAAMC,QAAO,GAAIF,gBAAgB,CAACL,IAAI;;MAEtC;MACA,MAAMC,QAAO,GAAI,EAAE;MACnB,KAAK,MAAMO,IAAG,IAAKD,QAAQ,EAAE;QAC3B,MAAME,YAAW,GAAI,MAAMV,KAAK,CAACO,GAAG,CAAE,wBAAuBE,IAAI,CAACE,UAAW,EAAC,CAAC;QAC/E,MAAMC,IAAG,GAAIF,YAAY,CAACT,IAAI;QAE9BC,QAAQ,CAACW,IAAI,CAAC;UACZF,UAAU,EAAEF,IAAI,CAACE,UAAU;UAC3BG,YAAY,EAAEL,IAAI,CAACK,YAAY;UAC/BF,IAAI,EAAEA;QACR,CAAC,CAAC;MACJ;;MAEA;MACAV,QAAQ,CAACa,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACL,IAAG,GAAII,CAAC,CAACJ,IAAI,CAAC;;MAExC;MACA,IAAI,CAACV,QAAO,GAAIA,QAAQ;IAC1B;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}